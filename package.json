  "scripts": {
    "web:dev:local": "vite --config vite.config.raw.ts --host --port 8100",

    "build:web:local": "tsc -b && vite build --config vite.config.raw.ts --mode=local",
    "build:android:local": "tsc -b && vite build --config vite.config.raw.ts --mode=android",

    "deploy:android-ota:local": "npm run build:android:local && capgo bundle zip",
    "deploy:android:local": "npm run build:android:local && cap sync android && cap run android",

    "install:sdk:local": "yalc add @afkae-brothers/as-seen-offline-db",
    "install:api:local": "openapi-generator-cli generate -i http://192.168.0.179:4001/api/reference/openapi.yaml -g typescript-fetch  -o ./src/api/as-seen-api",
    "install:shadcn:add": "shadcn@2.9.0 add",
    
    "android:dev:local": "npm run build:android:local && cap sync android && ionic cap run android -l --external --public-host 192.168.0.179 -- --config vite.config.raw.ts --mode=android"
